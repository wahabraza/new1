<body>
    <h1 style="color:red">@TempData["message"]</h1>
    <h1 style="color:red">@TempData["Sameuser"]</h1>
<div class="wrapper">
 
@Html.Partial("_header")
 
 
<section class="main-slider inner-slider">
<div class="slider">
<ul class="mslider-inner">
<li><img src="~/images/si1.jpg" alt="Fashion Responsive"/></li>
<li><img src="~/images/si2.jpg" alt="Fashion Responsive"/></li>
<li><img src="~/images/si3.jpg" alt="Fashion Responsive"/></li>
</ul>
</div>
<div class="main-slider-curve curve-img"></div>
</section>
 
 
<section id="register" class="register">
<div class="container">
<div class="row">
<div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
<div class="register-account">
<form method="post" action="/Login/add">
<ul>
<li>
<label>Username*</label>
<input type="text" class="form-control" name="username" placeholder="User Name" required>
</li>
</ul>
<ul class="row">
<li class="col-lg-6">
<label>First Name*</label>
<input type="text" class="form-control" name="firstname" placeholder="First Name" required>
</li>
<li class="col-lg-6">
<label>Last Name*</label>
<input type="text" class="form-control" name="lastname" placeholder="Last Name" required>
</li>
</ul>
<ul class="row">
<li class="col-lg-6">
<label>Email Address*</label>
<input type="Email" class="form-control" name="email" placeholder="Email" required>
</li>
<li class="col-lg-3">
<label>Password*</label>
<input type="password" class="form-control" name="password" placeholder="******" required>
</li>
<li class="col-lg-3">
<label>Confirm Password*</label>
<input type="password" class="form-control" name="confirmpassword" placeholder="******" required>
</li>
</ul>
<ul>
<li>
<label>Postal ADDRESS*</label>
<input type="text" class="form-control" name="address" placeholder="Address" required>
</li>
</ul>
<ul class="row">
<li class="col-lg-3">
<label>City*</label>
<input type="text" class="form-control" name="city" placeholder="City" required>
</li>
<li class="col-lg-3">
<label>Country*</label>
<input type="text" class="form-control" name="country" placeholder="Country" required>
</li>
<li class="col-lg-3">
<label>Zip Code*</label>
<input type="text" class="form-control" name="code" placeholder="Zip Code" required>
</li>
<li class="col-lg-3">
<label>Phone*</label>
<input type="text" class="form-control" name="phone" placeholder="Phone" required>
</li>
</ul>
<ul>
<li>
<input type="submit" name="submit" id="submit" value="Create Account" class="btn btn-info pull-right">
</li>
</ul>
</form>
</div>
</div>
<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
<div class="login-box">
<h3>Customer Login Area</h3>
<form role="form" class="login-form" method="post">
<div class="form-group">
<div class="input-group"> <span class="input-group-addon"><i class="fa fa-user"></i></span>
<input type="text" class="form-control" name="InputName" placeholder="Username" required>
</div>
</div>
<div class="form-group">
<div class="input-group"> <span class="input-group-addon"><i class="fa fa-lock"></i></span>
<input type="password" class="form-control" id="InputEmail" name="InputEmail" placeholder="Password" required>
</div>
</div>
<div class="form-bottom"> <span class="pull-left">
<input type="checkbox" checked="checked" id="checkbox4" class="css-checkbox">
<label class="css-label lite-gray-check" for="checkbox4">Remember Me </label>
</span>
<input type="submit" name="submit" value="Login" class="btn btn-info pull-right">
</div>
</form>
</div>
</div>
</div>
</div>
</section>
 
@Html.Partial("_footer")
</div>
 
 
<script src="~/js/jquery-1.11.1.min.js"></script> 
